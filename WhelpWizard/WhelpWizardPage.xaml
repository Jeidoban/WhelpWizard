<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:local="clr-namespace:WhelpWizard" x:Class="WhelpWizard.WhelpWizardPage">
	<StackLayout Orientation="Vertical">
        <Label 
            VerticalOptions="CenterAndExpand" 
            HorizontalOptions="Center" 
            Text="Whelp Wizard" 
            FontSize="30" 
        />
        <StackLayout Orientation="Horizontal">
            <Button Text="List of Dams" HorizontalOptions="Start" Margin="20, 20, 0, 10" Clicked="GoToListOfDams"/>
            <Button Text="Calculator" HorizontalOptions="EndAndExpand" Margin="0, 20, 20, 10" Clicked="GoToCalculator"/>
        </StackLayout>
    </StackLayout>
</ContentPage>
